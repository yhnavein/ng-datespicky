{
  "name": "ng-datespicky",
  "version": "0.1.0",
  "description": "AngularJS Date Range Picker written in TypeScript with no additional dependencies",
  "repository": {
    "type": "git",
    "url": "git@github.com:yhnavein/ng-datespicky.git"
  },
  "author": {
    "name": "Piotr Dabrowski @ PureDev"
  },
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --progress --watch",
    "htmlhint": "htmlhint --config .htmlhintrc src/**/*.html",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "test": "true",
    "test:watch": "jest --watch",
    "test-watch": "jest --watch",
    "test:ci": "jest --runInBand --coverage",
    "test-ci": "jest --runInBand --coverage",
    "test:coverage": "jest --coverage",
    "prepare": "npm run build"
  },
  "contributors": [
    "Izabela Ciskowska"
  ],
  "license": "MIT",
  "main": "./dist/datespicky.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/angular": "^1.6.40",
    "@types/jest": "^21.1.2",
    "angular": "1.6.9",
    "angular-mocks": "1.6.9",
    "file-loader": "0.11.2",
    "html-loader": "0.5.5",
    "htmlhint": "0.9.13",
    "htmlhint-loader": "1.3.0",
    "istanbul-instrumenter-loader": "3.0.0",
    "jest": "21.2.1",
    "moment": "2.20.1",
    "ng-annotate-loader": "0.6.1",
    "node-sass": "4.11.0",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-scss": "2.3.0",
    "stylelint-webpack-plugin": "0.10.1",
    "ts-jest": "21.1.2",
    "ts-loader": "2.3.7",
    "tslint": "5.9.1",
    "tslint-loader": "3.5.3",
    "typescript": "2.7.1",
    "url-loader": "0.6.2",
    "webpack": "3.11.0"
  },
  "peerDependencies": {
    "angular": "^1.6.5",
    "moment": "^2.18.1"
  },
  "dependencies": {}
}
